{
    "name": "laminas-api-tools/api-tools",
    "description": "Laminas API Tools module for Laminas",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "framework"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools/issues",
        "source": "https://github.com/laminas-api-tools/api-tools",
        "rss": "https://github.com/laminas-api-tools/api-tools/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "bshaffer/oauth2-server-php": "dev-develop",
        "laminas-api-tools/api-tools-api-problem": "~1.0-dev",
        "laminas-api-tools/api-tools-content-negotiation": "~1.0-dev",
        "laminas-api-tools/api-tools-content-validation": "~1.0-dev",
        "laminas-api-tools/api-tools-hal": "~1.0-dev",
        "laminas-api-tools/api-tools-mvc-auth": "~1.0-dev",
        "laminas-api-tools/api-tools-oauth2": "~1.0-dev",
        "laminas-api-tools/api-tools-provider": "~1.0-dev",
        "laminas-api-tools/api-tools-rest": "~1.0-dev",
        "laminas-api-tools/api-tools-rpc": "~1.0-dev",
        "laminas-api-tools/api-tools-versioning": "~1.0-dev",
        "laminas/laminas": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "rwoverdijk/assetmanager": "~1.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": ">=0.6.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\": "src/",
            "LaminasTest\\ApiTools\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "replace": {
        "zfcampus/zf-apigility": "self.version"
    }
}
